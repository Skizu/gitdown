{
    "name": "skizu/gitdown",
    "type": "library",
    "description": "Markdown parser for PHP based on the CommonMark spec with integration into git",
    "keywords": ["gitdown","markdown","parser","commonmark","git"],
    "homepage": "https://github.com/skizu/gitdown",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Daniel Hunt",
            "email": "email@danielhunt.info",
            "homepage": "http://danielhunt.info",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.4.8",
        "league/commonmark": "0.10.0",
        "gitonomy/gitlib": "v0.1.7"

    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    },
    "autoload": {
        "psr-4": {
            "Skizu\\GitDown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skizu\\GitDown\\Tests\\Functional\\": "tests/functional"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
